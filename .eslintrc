{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": [2, "always"],
    "eqeqeq": 2,
    "quotes": [2, "double"],
    "use-isnan": 2,
    "camelcase": 2,
    "curly": 2,
    "comma-spacing": 2,
    "guard-for-in": 2,
    "eol-last": 2,
    "indent": [2, 2, {
      "SwitchCase": 1
    }],
    "key-spacing": 0,
    "new-cap": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-alert": 0,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-empty": [2, { "allowEmptyCatch": true }],
    "no-extra-semi": 2,
    "no-invalid-regexp": 2,
    "no-undef": 2,
    "no-shadow": 0,
    "no-extend-native": 2,
    "no-shadow-restricted-names": 0,
    "no-redeclare": 2,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 2,
    "no-multi-spaces": 0,
    "no-extra-boolean-cast": 2,
    "no-unused-vars": 0,
    "no-use-before-define": 2,
    "no-trailing-spaces": 2,
    "no-constant-condition": 2,
    "no-duplicate-case": 2,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-ex-assign": 2,
    "no-irregular-whitespace": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-const-assign": 2,
    "no-empty-pattern": 2,
    "no-self-assign": 2,
    "valid-typeof": 2,
    "semi-spacing": 2,
    "space-infix-ops": 2,
    "strict": [0, "function"]
  }
}
